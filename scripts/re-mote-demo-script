; setup all sub systems

(go)

; we have 4 machines

(hosts)

; run a simple ls on them

(listing (hosts))

; all green! now lets apt update them

(aptdate (hosts))

; lets drill down into a single host

(log-hosts "re-a")

; redirect output to the repl

(redirect-output ["re-mote.log"])

; list

(listing (hosts))

; make things quite again

(reset)

; all quite now

(listing (hosts))
